{
  "name": "dynsense",
  "private": true,
  "version": "0.1.0",
  "description": "AI-native project management platform for consultancy firms",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean",
    "db:generate": "pnpm --filter @dynsense/db generate",
    "db:migrate": "pnpm --filter @dynsense/db migrate",
    "db:seed": "pnpm --filter @dynsense/db seed",
    "db:studio": "pnpm --filter @dynsense/db studio"
  },
  "devDependencies": {
    "turbo": "^2.8.10"
  }
}
